@{
    ViewData["Title"] = "Survey Result";
}
<h1>@ViewData["Title"]</h1>
<div id="info">
    <div id="data">
        <table>
            <tr>
                <td>Name:</td>
                <td><p>@ViewBag.Name</p></td>
            </tr>
            <tr>
                <td>Dojo Location:</td>
                <td><p>@ViewBag.Location</p></td>
            </tr>
            <tr>
                <td>Favorite Language:</td>
                <td><p>@ViewBag.Language</p></td>
            </tr>
            <tr>
                <td>Comment:</td>
                <td><p>@ViewBag.Comment</p></td>
            </tr>
        </table>
    </div>
    <a href="/">Go Back</a>
</div>
